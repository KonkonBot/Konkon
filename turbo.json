{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "start": {
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "interactive": true,
      "persistent": true,
      "cache": false
    }
  }
}