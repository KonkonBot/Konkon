{
   "$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
   "organizeImports": {
     "enabled": true
   },
   "linter": {
     "enabled": true,
     "rules": {
       "recommended": true,
       "correctness": {
        "noUnusedImports": "warn"
       }
     }
   },
   "formatter": {
     "enabled": true,
     "formatWithErrors": true,
     "indentWidth": 2,
     "indentStyle": "tab",
     "lineWidth": 100
   }
 }